#include <iostream>
#include <fstream>
using namespace std;

int main(int argc, char *argv[])
{
	if (argc == 1)
	{
		cout << "The arguement is not correct" << endl;
		cout << "Bye!";
		return -1;
	}
	ofstream fout(argv[1]);


	return 0;
}
